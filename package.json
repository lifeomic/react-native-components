{
  "name": "@lifeomic/react-native-components",
  "version": "0.0.0",
  "description": "React-native components for building a mobile app against the LifeOmic Platform",
  "scripts": {
    "format": "yarn prettier '**/*.{ts,tsx,js,json,md}'",
    "format:write": "yarn format --write",
    "format:check": "yarn format --check",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "test": "yarn lint && yarn jest",
    "test:ci": "yarn lint && yarn jest --coverage --runInBand --silent --ci --logHeapUsage",
    "build": "rm -rf dist && tsc -p tsconfig.build.json --outDir dist && cp package.json ./dist",
    "gql:fetch": "bin/gql introspection",
    "gql:codegen": "bin/gql codegen",
    "gql:sync": "yarn gql:fetch && yarn gql:codegen"
  },
  "author": "LifeOmic <development@lifeomic.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@apollo/client": "^3.7.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/fragment-matcher": "^4.0.0",
    "@graphql-codegen/named-operations-object": "^2.3.1",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-resolvers": "^3.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.1",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-native": "^0.71.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.33.0",
    "graphql": "^16.6.0",
    "jest": "^29.4.1",
    "metro-react-native-babel-preset": "^0.74.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-native": "0.68.2",
    "react-native-app-auth": "^6.4.3",
    "react-native-device-info": "^10.3.0",
    "react-native-keychain": "^8.1.1",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^19.0.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-native": ">=0.64.1",
    "react-native-app-auth": ">=6.4.3",
    "react-native-device-info": "^10.3.0",
    "react-native-keychain": "^8.1.1"
  },
  "dependencies": {}
}
