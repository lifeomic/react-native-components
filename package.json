{
  "name": "@lifeomic/react-native-components",
  "version": "0.0.0",
  "description": "React-native components for building a mobile app against the LifeOmic Platform",
  "scripts": {
    "format": "yarn prettier '**/*.{ts,tsx,js,json,md}'",
    "format:write": "yarn format --write",
    "format:check": "yarn format --check",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "test": "yarn jest",
    "test:ci": "yarn lint && yarn jest --coverage --runInBand --silent --ci --logHeapUsage",
    "build": "rm -rf dist && tsc -p tsconfig.build.json --outDir dist && cp package.json ./dist"
  },
  "author": "LifeOmic <development@lifeomic.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@lifeomic/eslint-plugin-i18next": "^2.0.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.1",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-native": "^0.71.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "metro-react-native-babel-preset": "^0.74.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-native": "0.68.2",
    "react-native-app-auth": "^6.4.3",
    "react-native-device-info": "^10.3.0",
    "react-native-keychain": "^8.1.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^19.0.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "i18next": "^22.4.9",
    "react": ">=17.0.1",
    "react-i18next": "^12.1.5",
    "react-native": ">=0.64.1",
    "react-native-app-auth": ">=6.4.3",
    "react-native-device-info": "^10.3.0",
    "react-native-keychain": "^8.1.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0"
  },
  "dependencies": {}
}
